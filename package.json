{
  "name": "@mjlangan/react-confirm-alert",
  "version": "2.1.0",
  "description": "A forked React component confirm dialog with improved accessibility.",
  "main": "lib/index.js",
  "scripts": {
    "dev": "webpack-dev-server --hot --inline",
    "test": "npm run lint && mocha ./test/**/*.test.js --compilers js:babel-register",
    "test:watch": "mocha ./test/**/*.test.js --compilers js:babel-register --watch",
    "lint": "eslint ./src",
    "clean": "rimraf lib",
    "build": "npm run clean && babel src --out-dir lib && npm run build:demo",
    "build:demo": "webpack --config webpack.config.production.js",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mjlangan/react-confirm-alert.git"
  },
  "keywords": [
    "react alert example",
    "redux confirm dialog",
    "confirm alert react js",
    "react-confirm-alert",
    "react-confirm",
    "confirm-dialog",
    "confirm",
    "dialog",
    "react"
  ],
  "author": "Michael Langan (michaell@allenai.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mjlangan/react-confirm-alert/issues"
  },
  "homepage": "https://github.com/mjlangan/react-confirm-alert",
  "files": [
    "lib",
    "src/react-confirm-alert.css"
  ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "css-loader": "^0.28.7",
    "eslint": "^4.5.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.3.0",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^3.5.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-hot-loader": "^1.3.1",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "styled-components": "^2.1.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  },
  "dependencies": {
    "focus-trap-react": "^4.0.0"
  },
  "peerDependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  }
